<body>

<%
int n1 = (int)(Math.random()*15)+1;
int n2 = (int)(Math.random()*15)+1;
%>

<table>
<%
for (int i = 0; i < n1; i++){
	%> 
	<th><%=n1 %></th>
	<%
	for (int j = 0; j>i; j++){
		%> 
		<tr><%=n2 %></tr>
		<%
	}
	%>
	<%
	}
	%>

</table>

</body>
