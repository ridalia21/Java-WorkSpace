	Contacto c2 = new Contacto("Pepe");

	if (!c2.agregaMail("pepe@gmail.com")) {
		out.print("<p>ya estaba agregado</p>");
	}
	
	if (!c1.agregaMail("pepe@rewr.es")) {
		out.print("<p>ya estaba agregado</p>");
	}
	if (!c1.agregaMail("pepe@yahoo.es")) {
		out.print("<p>ya estaba agregado</p>");
	}
	if (!c1.agregaTfno("914324325")) {
		out.print("<p>ya estaba agregado</p>");
	}

	out.print("<p>" + c1 + "</p>");

	if (!c1.eliminaMail("pepe@rewr.es")) {
		out.print("<p>No estaba agregado</p>");
	}

	out.print("<p>" + c1 + "</p>");
	%>
